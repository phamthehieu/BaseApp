{
  "name": "BaseApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start:dev": "cross-env ENVFILE=.env.development react-native start",
    "start:dev:reset": "cross-env ENVFILE=.env.development react-native start --reset-cache",
    "start:prod": "cross-env ENVFILE=.env.production react-native start",
    "start:prod:reset": "cross-env ENVFILE=.env.production react-native start --reset-cache",
    "build:android:dev": "react-native run-android --mode=debug",
    "build:android:prod": "react-native run-android --mode=release",
    "build:ios:dev": "cross-env ENVFILE=.env.development react-native run-ios --configuration Debug",
    "build:ios:prod": "cross-env ENVFILE=.env.production react-native run-ios --configuration Release",
    "test": "jest"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-config": "^1.6.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-mmkv": "^4.1.1",
    "react-native-nitro-modules": "^0.32.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.19.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
